{
  "name": "base-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "dev": "nodemon -r --exec ts-node ./src/index.ts",
    "build:debug": "tsc -p ./tsconfig.json",
    "build:dev": "nodemon -r --exec ts-node ./src/index.ts",
    "build:prod": "tsc -p ./tsconfig.json && echo 'npm run copyfiles'",
    "build:start": "yarn clean && yarn build:prod && yarn launchProd",
    "migration:generate": "npx sequelize migration:generate --migrations-path 'sequelize/migrations' --name 'create-models'",
    "db:migrate": "npx sequelize db:migrate --config 'sequelize/config.dev.json' --migrations-path 'sequelize/migrations'",
    "db:migrate:undo:all": "npx sequelize db:migrate:undo:all --config 'sequelize/config.dev.json' --migrations-path 'sequelize/migrations'",
    "db:push": "yarn db:migrate:undo:all && yarn db:migrate"
  },
  "keywords": [],
